		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.digester" rev="1.6.0" conf="compile->compile"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.logging" rev="1.0.4" conf="compile->compile"/>
