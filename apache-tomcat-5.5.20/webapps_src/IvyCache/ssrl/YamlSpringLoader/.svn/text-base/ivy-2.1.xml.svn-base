<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd" xmlns:e="http://ant.apache.org/ivy/extra">
    <info organisation="ssrl" module="YamlSpringLoader" revision="2.1" status="release" publication="20110125011039"> 
	</info>        
	    
   <configurations>
		<conf name="runtime" description="provides factory for yaml spring beans"/>
      	<conf name="test" extends="runtime" visibility="private" description="for testing"/>
    </configurations>

    <publications>
      <artifact name="YamlSpringLoader" type="jar" ext="jar"/> 
      
     </publications>
 
	<dependencies>
		<dependency org="org.springframework" name="org.springframework.beans" rev="3.0.1.RELEASE-A">
			<exclude org="org.codehaus.groovy" name="com.springsource.org.codehaus.groovy"/>
		</dependency>
		<dependency org="org.springframework" name="org.springframework.context" rev="3.0.1.RELEASE-A">
			<exclude org="org.codehaus.groovy" name="com.springsource.org.codehaus.groovy"/>
		</dependency>
		<dependency org="org.yaml" name="snakeyaml" rev="1.7"/>
		<dependency org="junit" name="junit" rev="3.8"/>		
		<dependency org="log4j" name="log4j" rev="1.2.15"/>		
		<dependency org="commons-lang" name="commons-lang" rev="2.5"/>
		<dependency org="commons-io" name="commons-io" rev="1.4"/>		
	</dependencies>   
	    
</ivy-module>
