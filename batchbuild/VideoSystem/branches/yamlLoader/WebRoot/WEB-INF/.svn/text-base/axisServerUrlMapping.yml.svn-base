beans:

  - id: noVideoJpeg
    className: videoSystem.beans.JpegBeanFactory
    properties:
      filename: novideo.jpg

  - id: passwordFactory
    className: videoSystem.util.PasswordFileReader
    singleton: true
    properties:
      passwordFile: /usr/local/dcs/passwords/axis.txt,/usr/local/tomcat/access/axis.txt

  - id: VideoFilter
    className: videoSystem.video.source.VideoFilter
    properties:
      streamKeepAliveTimeMs: 35000
      nullImage: !ref noVideoJpeg
 
  - id: VideoStandard
    className: videoSystem.beans.AxisServer
    abztract: true
    properties:
      smallSize: resolution=176x112
      mediumSize: resolution=352x240
      largeSize: resolution=704x480
      low: compression=100
      medium: compression=50
      high: compression=0
      timeout: 6000
      streamKeepAliveTimeMs: 5000
      nullImage: !ref noVideoJpeg
      ptzClass: videoSystem.video.ptz.PtzControlAxis2400 
      passwordFactory: !ref passwordFactory

  - id : Axis2110
    parent: VideoStandard
    abztract: true
    properties:
      smallSize: resolution=320x240
      mediumSize: resolution=320x240
      largeSize: resolution=640x480
      imageServletName: /cgi-bin/mjpg/video.cgi
      sleepTimeBetweenImagesMs: 1

  - id : Axis2120
    parent: VideoStandard
    abztract: true
    properties:
      smallSize: resolution=352x240
      mediumSize: resolution=352x240
      largeSize: resolution=704x480
      imageServletName: /cgi-bin/mjpg/video.cgi
      sleepTimeBetweenImagesMs: 1


  - id: Axis241Q
    parent: VideoStandard
    abztract: true
    properties:
      smallSize: resolution=176x112
      mediumSize: resolution=352x240
      largeSize: resolution=704x480
      imageServletName: /axis-cgi/mjpg/video.cgi
      sleepTimeBetweenImagesMs: 2

  - id: Axis210
    parent: VideoStandard
    abztract: true
    properties:
      imageServletName: /axis-cgi/mjpg/video.cgi
      sleepTimeBetweenImagesMs: 1

