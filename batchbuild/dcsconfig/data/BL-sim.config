##############################
# BL-sim config
##############################

# dcs
dcs.rootDir=/usr/local/dcs

# dcss server
dcss.host=smbdev3.slac.stanford.edu

#serverPorts.txt file fields conversion table
#   1st -->dcss.scriptPort 
#   2nd -->dcss.hardwarePort
#   3rd -->dcss.guiPort
dcss.scriptPort=14341
dcss.hardwarePort=14342
dcss.guiPort=14343
dcss.ssl=1
dcss.authProtocol=2
dcss.certificate=/usr/local/dcs/dcsconfig/data/BL-sim.crt

dcss.binary_message_location=/data/blctl/currentBinaryMessage

##################################################
# define it if you want DCSS update XFORMSTATUSFILE
# it should be a file on NFS so all adxv can monitor it
##################################################
adxv.xformstatusfile=/data/blctl/XFORMSTATUSFILE.BL-sim

dcss.forcedDoor=closed
# dcss displays
dcss.display=hutch dontcare smbdev1:0.0 #virtual Hutch
dcss.display=local dontcare biotest.slac.stanford.edu :0.0
dcss.display=local dontcare smblx10.slac.stanford.edu :0.0
dcss.dir=/tmp
# how often to validate session ids (in msec)
dcss.validationRate = 3000

# Location of the dabase memory map file
dcss.dbMapFile=/usr/local/dcs/dcss/dbmapfile/BL-sim.dat
dcss.stringFileDir=/usr/local/dcs/dcss/dbmapfile/BL-simString


# dcss logging: 
# To turn off stdout logging, set logStdout to false
# To turn off udp log, set logUdpHost or logUdpPort to an empty string.
# To turn off log file, set logFilePattern or logFileSize or logFileMax to an empty string
# logLevel can be OFF, SEVERE, WARNING, INFO, CONFIG, FINE, FINER, FINEST OR ALL.
dcss.logStdout=true
dcss.logUdpHost=
dcss.logUdpPort=
dcss.logFilePattern=./blsim_dcss_log_%g_%u.log
dcss.logFileSize=31457280
dcss.logFileMax=20
dcss.logLevel=ALL
dcss.logLibs=auth_client|http_cpp

# Dumping database.dat to text file. Set dbDumpFlag=false to turn it off.
dcss.dbDumpFlag=true
dcss.dbDumpFile=/home/webserverroot/servlets/webice/data/dcs/BL-sim.dump
dcss.dbDumpRate=10000

bluice.hutchView=DCS::HutchOverviewBL12-2
# blueice
bluice.tabOrder=Hutch Sample Collect Screening LCLSL614 LCLSCrystal LCLSGrid Users UserLog Staff
bluice.videoTabList=COMBO_SAMPLE_ONLY COMBO_INLINE_ONLY Hutch Panel Diffraction
bluice.deviceDefinitionFilename=
bluice.mirrorView=
bluice.monoView=
bluice.defaultHost=smbdev3
bluice.useOneTimeTicket=1
bluice.beamlineView=detectorPosition goniometer table frontEndSlits frontEndApertures mirrorView toroid hutchOverview monoView

dhs.instance=camera axis2400 /usr/local/dcs/BL-sim/camera_%g_%u.log /usr/local/dcs/BL-sim/camera.dat 500 5000 5000

dhs.logStdout=TRUE
dhs.logFileSize=31457280
dhs.logFileMax=10
dhs.logLevel=INFO

axis2400.hostname=bl92aaxis
axis2400.port=8000
axis2400.passwordFile=/usr/local/dcs/BL-sim/axis2400Password.txt
axis2400.url_path=/axis-cgi/jpg/image.cgi?camera=2&clock=0&date=0&text=0 HTTP/1.0\r\n

# imperson dhs
impdhs.name=imperson
impdhs.tmpDir=/tmp
impdhs.choochBinDir=/usr/local/dcs/autochooch/install/bin
impdhs.choochDatDir=/usr/local/dcs/autochooch/install/data
impdhs.cameraHost=smb.slac.stanford.edu
impdhs.cameraPort=80
impdhs.impHost=localhost
impdhs.impPort=61001
impdhs.silHost=smb.slac.stanford.edu
impdhs.silPort=80
impdhs.latestEventIdUrl=/crystals/getLatestEventId.do
impdhs.cassetteDataUrl=/crystals/getCassetteData.do

