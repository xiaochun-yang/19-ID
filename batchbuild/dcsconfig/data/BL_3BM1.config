##############################
# BL_3BM1.config
##############################

# dcss server.
dcss.host=10.109.2.21
dcss.dbMapFile=/usr/local/dcs/dcss/dbmapfile/BL_3BM1.dat

#serverPorts.txt file fields conversion table
#   1st -->dcss.scriptPort 
#   2nd -->dcss.hardwarePort
#   3rd -->dcss.guiPort
dcss.scriptPort=14244
dcss.hardwarePort=14242
dcss.guiPort=14243
dcss.authProtocol=2

# how often to validate session ids (in msec)
dcss.validationRate=30000

# dcss logging: 
# To turn off stdout logging, set logStdout to false
# To turn off udp log, set logUdpHost or logUdpPort to an empty string.
# To turn off log file, set logFilePattern or logFileSize or logFileMax to an empty string
# logLevel can be OFF, SEVERE, WARNING, INFO, CONFIG, FINE, FINER, FINEST OR ALL.
dcss.logStdout=true
dcss.logUdpHost=
dcss.logUdpPort=
dcss.logFilePattern=./bl_3BM1_dcss_log_%g_%u.txt
#dcss.logFilePattern=
dcss.logFileSize=31457280
dcss.logFileMax=20
dcss.logLevel=ALL
dcss.logLibs=

# Dumping database.dat to text file. Set dbDumpFlag=false to turn it off.
dcss.dbDumpFlag=false
dcss.dbDumpFile=../../dcsconfig/data/BL_3BM1.dump
dcss.dbDumpRate=10000

dcss.forcedDoor=closed

# override the default config in default.config
# authentication server
auth.host=10.109.2.21
auth.port=17000
auth.secureHost=10.109.2.21
auth.securePort=17001

#bluice.useSsl=0
#bluice.useRobot=0
bluice.tabOrder=Hutch Sample Scan Collect Users UserLog Staff
#bluice.deviceDefinitionFilename=
#bluice.mirrorView=
bluice.hutchView=DCS::HutchOverviewBL3BM1
bluice.monoView=DoubleCrystalMonoView
bluice.toroidView=ToroidView
bluice.beamlineView=goniometer detectorPosition mardtb

dcss.display=local dontcare smblx5.slac.stanford.edu:0.0 #Beamline console
dcss.display=local smblx5.slac.stanford.edu :0.0 #Beamline console

simdetector.name=detector
simdetector.imageDir=/tmp/images
simdetector.imageFilter=*.jpg

# image server
imgsrv.host=10.109.2.21
imgsrv.guiPort=14005
imgsrv.webPort=14006
imgsrv.httpPort=14007
imgsrv.tmpDir=/tmp
imgsrv.maxIdleTime=60
imgsrv.logStdout=true
imgsrv.logUdpHost=
imgsrv.logUdpPort=
imgsrv.logFilePattern=./imgsrv_log_%d.log
imgsrv.logFileSize=31457280
imgsrv.logFileMax=1
imgsrv.logLevel=ALL
imgsrv.logLibs=

# EPICS gateway
epicsgw.name=epics_gateway
epicsgw.default.UpdateRate=0
epicsgw.String.UpdateRate=10
epicsgw.Motor.UpdateRate=10

shutter.Al_1=1
shutter.Al_2=2
shutter.Al_4=3
shutter.Al_8=4
shutter.Al_16=5
shutter.Al_32=6
shutter.Se=7
shutter.Al_0.5=8

# impersonation server
# This is to demonstrate that we can 
# override the default config in default.config
imperson.host=10.109.2.21
imperson.port=61001

# imperson dhs
impdhs.name=imperson
impdhs.tmpDir=/tmp
impdhs.choochBinDir=/usr/local/dcs/autochooch/install/bin
impdhs.choochDatDir=/usr/local/dcs/autochooch/install/data
impdhs.cameraHost=smb.slac.stanford.edu
impdhs.cameraPort=80
impdhs.impHost=10.109.2.21
impdhs.impPort=61001
impdhs.silHost=10.109.2.21
impdhs.silPort=8480
impdhs.latestEventIdUrl=/crystals-dev/getLatestEventId.do
impdhs.cassetteDataUrl=/crystals-dev/getCassetteData.do

dhs.logStdout=TRUE
dhs.logFileSize=31457280
dhs.logFileMax=10
dhs.logLevel=INFO
mar345.commandDirectory=/logs/root
dhs.instance=detector marccd /usr/local/dcs/BL_3BM1/detector_%g_%u.log /usr/local/dcs/BL_3BM1/detector.dat 500 1000 200
marccd.beamCenter=162.5 162.5
marccd.serialNumber=1
marccd.hostname=10.109.2.24
marccd.commandPort=2222
marccd.darkRefreshTime=7200
marccd.darkExposureTolerance=.10
marccd.writeRawImages=F
marccd.impHost=10.109.2.21
marccd.impPort=61001
marccd.useImperson=T
marccd.collectDark=F
marccd.recordI0=F
marccd.detectorType=MAR165
#if it is not 0, port probing force results are in millimeter of port error
#if it is 0, the port probing force results are lbs-in, raw
robot.probeScaled=1

