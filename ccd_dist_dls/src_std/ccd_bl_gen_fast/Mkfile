CC = cc
CFLAGS = $(CFLAGS_OS) -D$(OSTYPE)
IOFLAGS = 
OEXTS = ccd_bl.o ccd_bl_input.o ccd_bl_gen.o ccd_bl_stat.o ccd_bl_glob.o util_misc.o
OBS  =
OBJS = $(OEXTS) $(OBS) ../../lib/$(OSTYPE)/auxlib.a
DD = ../../bin/$(OSTYPE)

PRCMD = pr

$(DD)/ccd_bl_gen_fast : $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) $(LDFLAGS) $(IOFLAGS) -o $(DD)/ccd_bl_gen_fast

$(OEXTS) :

clean :	$(OEXTS) $(OBS)
	rm $?

print :	*.c *.h Makefile
	$(PRCMD) $?
	touch print
