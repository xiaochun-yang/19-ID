# dcs
dcs.rootDir=/usr/local/dcs

# dcss server
dcss.host=blctl111.slac.stanford.edu
dcss.dbMapFile=/usr/local/dcs/dcss/dbmapfile/BL11-1.dat

#serverPorts.txt file fields conversion table
#   1st -->dcss.scriptPort 
#   2nd -->dcss.hardwarePort
#   3rd -->dcss.guiPort
dcss.ssl=1
dcss.scriptPort=14244
dcss.hardwarePort=14242
dcss.guiPort=14243
dcss.authProtocol=2
dcss.validateRate = 1000
dcss.certificate=/usr/local/dcs/dcsconfig/data/BL11-1.crt

#bluice.deviceDefinitionFilename=/usr/local/dcs/dcsconfig/data/BL11-1.dat
bluice.mirrorView=DownwardMirrorView
bluice.monoView=SingleCrystalHorizFocusMonoView

bluice.beamlineView=detectorPosition goniometer table frontEndApertures mirrorView monoView 
