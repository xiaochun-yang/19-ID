##############################
# SIM12-2 config
##############################

#####################################################
# SIM12-2 needs special care in distance and beamstop
#####################################################
### for normal user
dcss.preCheckMotorList=energy map_beam_size attenuation
#dcss.preCheckMotorList=attenuation
run.beam_width=beam_size_sample_x
run.beam_height=beam_size_sample_y
#run.energy=energy
run.distance=detector_z
run.beam_stop=beamstop_z
#run.phi=gonio_phi
#run.omega=gonio_omega
run.vert=detector_center_vert_offset
run.horz=detector_center_horz_offset

# dcs
dcs.rootDir=/usr/local/dcs

# dcss server
dcss.host=smbdev3.slac.stanford.edu

#serverPorts.txt file fields conversion table
#   1st -->dcss.scriptPort 
#   2nd -->dcss.hardwarePort
#   3rd -->dcss.guiPort
dcss.ssl=1
dcss.scriptPort=14331
dcss.hardwarePort=14332
dcss.guiPort=14333
dcss.authProtocol=2
dcss.certificate=/usr/local/dcs/dcsconfig/data/SIM12-2.crt

dcss.forcedDoor=closed
# dcss displays
dcss.display=hutch dontcare smbdev1:0.0 #virtual Hutch
dcss.display=local dontcare biotest.slac.stanford.edu :0.0
dcss.display=local dontcare smblx10.slac.stanford.edu :0.0
dcss.dir=/tmp
# how often to validate session ids (in msec)
dcss.validationRate = 3000

# Location of the dabase memory map file
dcss.dbMapFile=/usr/local/dcs/dcss/dbmapfile/SIM12-2.dat

bluice.tabOrder=Hutch Sample Collect Screening Scan Rastering Sorting Users UserLog Staff
bluice.videoTabList=COMBO_SAMPLE_ONLY COMBO_INLINE_ONLY Hutch Panel Diffraction

bluice.mirrorView=DownwardMirrorView
bluice.monoView=SingleCrystalHorizFocusMonoView
bluice.beamlineView=detectorPosition goniometer table frontEndSlits frontEndApertures mirrorView toroid hutchOverview monoView

# dcss logging: 
# To turn off stdout logging, set logStdout to false
# To turn off udp log, set logUdpHost or logUdpPort to an empty string.
# To turn off log file, set logFilePattern or logFileSize or logFileMax to an empty string
# logLevel can be OFF, SEVERE, WARNING, INFO, CONFIG, FINE, FINER, FINEST OR ALL.
dcss.logStdout=true
dcss.logUdpHost=
dcss.logUdpPort=
dcss.logFilePattern=./sim12-2_dcss_log_%g_%u.log
dcss.logFileSize=31457280
dcss.logFileMax=20
dcss.logLevel=ALL
dcss.logLibs=auth_client|http_cpp

# Dumping database.dat to text file. Set dbDumpFlag=false to turn it off.
dcss.dbDumpFlag=true
dcss.dbDumpFile=/home/webserverroot/servlets/webice/data/dcs/SIM12-2.dump
dcss.dbDumpRate=10000

bluice.hutchView=DCS::HutchOverviewBL12-2
# blueice
bluice.deviceDefinitionFilename=/usr/local/dcs/dcsconfig/data/SIM12-2.dat
bluice.mirrorView=
bluice.monoView=
bluice.defaultHost=bl122hutch


# imperson dhs
impdhs.name=imperson
impdhs.tmpDir=/tmp
impdhs.choochBinDir=/usr/local/dcs/autochooch/install/bin
impdhs.choochDatDir=/usr/local/dcs/autochooch/install/data
impdhs.cameraHost=smb.slac.stanford.edu
impdhs.cameraPort=80
impdhs.impHost=localhost
impdhs.impPort=61001

# Simulation Detector
simdetector.name=detector
# Default source image dir
#simdetector.imageDir=/data/blctl/simdetector_images/SIM12-2
simdetector.imageDir=/data/blctl/simdetector_images/BL-sim
# File containing a list of screening result directories
simdetector.cassetteFile=/usr/local/dcs/simdetector/linux/cassette_dirs.txt
simdetector.impHost=localhost
simdetector.impPort=61001

# for DCSS sampleVideo snapshot
video.snapshotUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?stream=BL12-2_SAMPLE&size=medium&resolution=high
video.snapshotDirectUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?stream=BL12-2_SAMPLE&size=medium&resolution=high
video.snapshotDirectInlineUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?method=getVideoImage&stream=BL12-2_INLINE&size=medium&resolution=high

video1.imageUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?method=getVideoImage&stream=BL12-2_SAMPLE
video2.imageUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?method=getVideoImage&stream=BL12-2_HUTCH
video3.imageUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?method=getVideoImage&stream=BL12-2_ROBOT
video4.imageUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?method=getVideoImage&stream=BL12-2_PANEL
video5.imageUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/video.html?method=getVideoImage&stream=BL12-2_INLINE

#presets
video1.presetRequestUrl=
video2.presetRequestUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=getPresetList&stream=BL12-2_HUTCH
video3.presetRequestUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=getPresetList&stream=BL12-2_ROBOT
video4.presetRequestUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=getPresetList&stream=BL12-2_PANEL

video1.moveRequestUrl=
video2.moveRequestUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=gotoPreset&stream=BL12-2_HUTCH&presetName=
video3.moveRequestUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=gotoPreset&stream=BL12-2_ROBOT&presetName=
video4.moveRequestUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=gotoPreset&stream=BL12-2_PANEL&presetName=

video1.textUrl=
video2.textUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=changeText&stream=BL12-2_HUTCH&text=
video3.textUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=changeText&stream=BL12-2_ROBOT&text=
video4.textUrl=http://smbws2.slac.stanford.edu:8080/VideoSystem/control.html?method=changeText&stream=BL12-2_PANEL&text=

#more arguments
video1.args=
video2.args=&camera=1
video3.args=&camera=3
video4.args=&camera=1


epicsgw.name=epics_gateway
#### update rate is ticks; 1 tick is about 0.1 seconds
#### 0 means immediately update up on epics
#epicsgw.default.UpdateRate=0
#class wide rate
epicsgw.String.UpdateRate=10
epicsgw.Motor.UpdateRate=10
#epicsgw.Shutter.UpdateRate=10
# object wide rate
#epicsgw.epics_string1.UpdateRate=0
#epicsgw.epics_motor1.UpdateRate=10
#####string for EPICS array forced read rate per object#####
#epicsgw.epics_string3.ForcedReadRate=10

attenuation.filterThickness=Al_64 1600 Al_32 800 Al_16 400 Al_8 200 Al_4 100 Al_2 50 Al_1 25 Al_0.5 12
bluice.filterLabelMap=Al_0.5 Al_0.5 Al_1 1 Al_2 2 Al_4 4 Al_8 8 Al_16 16 Al_32 32 Al_64 64 Se Se cb1 cb1 cb2 cb2 cb3 cb3

##############################################################
# preamplifier settings
# one set has 4 string values
# one set per harmonic
##############################################################
harmonic.0.amp_i_mirror_feedback_lower=0 lower
harmonic.0.amp_i_mirror_feedback_upper=0 upper
harmonic.0.amp_i_mirror_feedback_ssrl=0 ssrl
harmonic.0.amp_i_mirror_feedback_spear=0 spear
###
harmonic.1.amp_i_mirror_feedback_lower=1 lower
harmonic.1.amp_i_mirror_feedback_upper=1 upper
harmonic.1.amp_i_mirror_feedback_ssrl=1 ssrl
harmonic.1.amp_i_mirror_feedback_spear=1 spear
###
harmonic.2.amp_i_mirror_feedback_lower=2 lower
harmonic.2.amp_i_mirror_feedback_upper=2 upper
harmonic.2.amp_i_mirror_feedback_ssrl=2 ssrl
harmonic.2.amp_i_mirror_feedback_spear=2 spear

### digital output (board, channel)
mirror.hold.control=0 0

#########################################
#BluIce will show "Energy Config"
#if it found this
#The list must match with "energy_component_list" in the code.
# You can have different name for each checkbox
# the items must be in ONE_WORD due to itk widgets name limit
#########################################
energy.config=move_undulator_gap jump_harmonic move_table_vert move_mirror_vert adjust_detector_vert set_preamplifiers set_detector_threshold
show.energy_offset=1
show.gap_owner=1
show.lvdt=1


# FOR TESTING ONLY
screening.crystalAnalyzeImageUrl=https://smbws2.slac.stanford.edu:8445/crystal-analysis-server/analyzeImage.do
screening.centerAnalyzeImageUrl=https://smbws2.slac.stanford.edu:8445/crystal-analysis-server/analyzeImageCenter.do
screening.crystalAutoindexUrl=https://smbws2.slac.stanford.edu:8445/crystal-analysis-server/strategy.do?strategyMethod=mosflm

## this one may be removed if we decide merge it with normal autoindex.
screening.prepareReOrientUrl=https://smbws2.slac.stanford.edu:8445/crystal-analysis-server/strategy.do?strategyMethod=mosflm
# Second screening pass
screening.reOrientUrl=https://smbws2.slac.stanford.edu:8445/crystal-analysis-server/reautoindex.do?strategyMethod=mosflm

strategy.statusUrl=https://smbws2.slac.stanford.edu:8445/crystal-analysis-server/getDcsStrategy.do

### light control (board_index channel_index)
##back light is DigitalOutput
light.back=1 1
##side light is AnalogOutput
light.side=1 0
# put the light in and on
light.inline_on=1 2
# dimmer
light.inline_dim=1 1

bluice.alignFrontEndView=alignFrontEndViewBL12-2
