##############################
# SMBLX4 config
##############################

# dcs
dcs.rootDir=/user/local/dcs

# dcss server
dcss.host=smblx5.slac.stanford.edu

#serverPorts.txt file fields conversion table
#   1st -->dcss.scriptPort 
#   2nd -->dcss.hardwarePort
#   3rd -->dcss.guiPort
dcss.scriptPort=14341
dcss.hardwarePort=14342
dcss.guiPort=14343
dcss.authProtocol=2

dcss.forcedDoor=closed
# dcss displays
dcss.display=hutch dontcare smbdev1:0.0 #virtual Hutch
dcss.display=local dontcare biotest.slac.stanford.edu :0.0
dcss.display=local dontcare smblx10.slac.stanford.edu :0.0

dcss.dir=/tmp
# how often to validate session ids (in msec)
dcss.validationRate = 3000

# dcss logging: 
# To turn off stdout logging, set logStdout to false
# To turn off udp log, set logUdpHost or logUdpPort to an empty string.
# To turn off log file, set logFilePattern or logFileSize or logFileMax to an empty string
# logLevel can be OFF, SEVERE, WARNING, INFO, CONFIG, FINE, FINER, FINEST OR ALL.
dcss.logStdout=true
dcss.logUdpHost=
dcss.logUdpPort=
dcss.logFilePattern=./blsim_dcss_log_%g_%u.log
dcss.logFileSize=31457280
dcss.logFileMax=20
dcss.logLevel=ALL
dcss.logLibs=auth_client|http_cpp

# Dumping database.dat to text file. Set dbDumpFlag=false to turn it off.
dcss.dbDumpFlag=false
dcss.dbDumpFile=/home/webserverroot/servlets/webice/data/dcs/SMBLX4.dump
dcss.dbDumpRate=10000

# blueice
bluice.deviceDefinitionFilename=../dcsconfig/data/SMBLX4.dat
bluice.mirrorView=
bluice.monoView=
bluice.defaultHost=smblx5

# imperson dhs
impdhs.name=imperson
impdhs.tmpDir=/tmp
impdhs.choochBinDir=/usr/local/dcs/autochooch/install/bin
impdhs.choochDatDir=/usr/local/dcs/autochooch/install/data
impdhs.cameraHost=smb.slac.stanford.edu
impdhs.cameraPort=80
impdhs.impHost=blcpu4.slac.stanford.edu
impdhs.impPort=61001
impdhs.silHost=smb.slac.stanford.edu
impdhs.silPort=8480
impdhs.latestEventIdUrl=/crystals-dev/getLatestEventId.do
impdhs.cassetteDataUrl=/crystals-dev/getCassetteData.do

# Simulation Detector
simdetector.name=simdetector
# mode can be screening or collecting
#simdetector.mode=screening
simdetector.mode=collecting
# imageFilter is used when mode is collecting
simdetector.imageFilter=*.img
# image dir must end with a dir name that matches
# the screening root directory.
# For example:
# imageDir = /<any>/<any>/xxx
# screening root dit = /<any>/xxx
#simdetector.imageDir=/data/blctl/screening_data/cassette92
#simdetector.imageDir=/data/penjitk/screening_data/ana/VINCULIN
#simdetector.imageDir=/data/penjitk/jcsg/screening/cassette130
#simdetector.imageDir=/data/ana/remove/snapshot
#simdetector.imageDir=/data/blctl/screening_data/cassette132
#simdetector.imageDir=/data/ana/rath/snapshot
#simdetector.imageDir=/data/ana/rath/9_2
simdetector.imageDir=/data/blctl/simdetector_images
# image file extension must match
# the detectorType (Q315CCD or MAR325) set by the dcss (see dcss dump file).
# For mar325 the image type is .mccd.
#simdetector.imageExt=.mccd
simdetector.imageExt=.img


