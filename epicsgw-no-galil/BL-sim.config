##############################
# BL-sim config
##############################

# dcs
dcs.rootDir=/usr/local/dcs

# dcss server
dcss.host=smbdev2.slac.stanford.edu

#serverPorts.txt file fields conversion table
#   1st -->dcss.scriptPort 
#   2nd -->dcss.hardwarePort
#   3rd -->dcss.guiPort
dcss.scriptPort=14341
dcss.hardwarePort=14342
dcss.guiPort=14343
dcss.ssl=1
dcss.authProtocol=2
dcss.certificate=/usr/local/dcs/dcsconfig/data/BL-sim.crt

##################################################
# define it if you want DCSS update XFORMSTATUSFILE
# it should be a file on NFS so all adxv can monitor it
##################################################
adxv.xformstatusfile=/data/blctl/XFORMSTATUSFILE.BL-sim

dcss.forcedDoor=closed
# dcss displays
dcss.display=hutch dontcare smbdev1:0.0 #virtual Hutch
dcss.display=local dontcare biotest.slac.stanford.edu :0.0
dcss.display=local dontcare smblx10.slac.stanford.edu :0.0
dcss.dir=/tmp
# how often to validate session ids (in msec)
dcss.validationRate = 3000

# Location of the dabase memory map file
dcss.dbMapFile=/usr/local/dcs/dcss/dbmapfile/BL-sim.dat


# dcss logging: 
# To turn off stdout logging, set logStdout to false
# To turn off udp log, set logUdpHost or logUdpPort to an empty string.
# To turn off log file, set logFilePattern or logFileSize or logFileMax to an empty string
# logLevel can be OFF, SEVERE, WARNING, INFO, CONFIG, FINE, FINER, FINEST OR ALL.
dcss.logStdout=true
dcss.logUdpHost=
dcss.logUdpPort=
dcss.logFilePattern=./blsim_dcss_log_%g_%u.log
dcss.logFileSize=31457280
dcss.logFileMax=20
dcss.logLevel=ALL
dcss.logLibs=auth_client|http_cpp

# Dumping database.dat to text file. Set dbDumpFlag=false to turn it off.
dcss.dbDumpFlag=true
dcss.dbDumpFile=/home/webserverroot/servlets/webice/data/dcs/BL-sim.dump
dcss.dbDumpRate=10000

#####
cryojetDhs.name=cryojetDhs
cryojetDhs.serialPort=/dev/ttyS1

# blueice
bluice.deviceDefinitionFilename=../dcsconfig/data/BL-sim.dat
bluice.mirrorView=
bluice.monoView=
bluice.defaultHost=smbdev2
bluice.useOneTimeTicket=1

# imperson dhs
impdhs.name=imperson
impdhs.tmpDir=/tmp
impdhs.choochBinDir=/usr/local/dcs/autochooch/install/bin
impdhs.choochDatDir=/usr/local/dcs/autochooch/install/data
impdhs.cameraHost=smb.slac.stanford.edu
impdhs.cameraPort=80
impdhs.impHost=localhost
impdhs.impPort=61001
impdhs.silHost=smb.slac.stanford.edu
impdhs.silPort=80
impdhs.latestEventIdUrl=/crystals/getLatestEventId.do
impdhs.cassetteDataUrl=/crystals/getCassetteData.do

# Simulation Detector
simdetector.name=simdetector
# Default source image dir
simdetector.imageDir=/data/blctl/simdetector_images
# Use default values from imperson. host and imperson. port
#simdetector.impHost=blcpu5.slac.stanford.edu
#simdetector.impPort=61001

# video urls
video1.imageUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/BluIceImageStream?beamline=9-2&camera=sample
video2.imageUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/BluIceImageStream?beamline=9-2&camera=hutch
video3.imageUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/BluIceImageStream?beamline=9-2&camera=fixed
video4.imageUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/BluIceImageStream?beamline=9-2&camera=control

# for DCSS sampleVideo snapshot
video.snapshotUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video1/axis-cgi/jpg/image.cgi?camera=2

#presets, needs the channel arguments
video1.presetRequestUrl=
video2.presetRequestUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video1/axis-cgi/com/ptz.cgi?query=presetposcam
video3.presetRequestUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video1/axis-cgi/com/ptz.cgi?query=presetposcam
video4.presetRequestUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video2/axis-cgi/com/ptz.cgi?query=presetposcam

#url for requesting a move, needs the channel arguments
video1.moveRequestUrl=
video2.moveRequestUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video1/axis-cgi/com/ptz.cgi
video3.moveRequestUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video1/axis-cgi/com/ptz.cgi
video4.moveRequestUrl=http://smb.slac.stanford.edu/BluIceVideo/bl92/video2/axis-cgi/com/ptz.cgi

#more arguments
video1.args=
video2.args=&camera=1
video3.args=&camera=3
video4.args=&camera=1

#change text within image needs the channel arguments
video1.textUrl=
video2.textUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/ImageText?location=9-2:hutch
video3.textUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/ImageText?location=9-2:hutch
video4.textUrl=http://smb.slac.stanford.edu:8080/imagetext/servlet/ImageText?location=9-2:control

